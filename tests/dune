(executable
 (name extract_tests)
 (libraries str)
 (modules extract_tests))

(rule
 (with-stdout-to
  dune.inc.new
  (run ./extract_tests.exe -write-dune-file)))

(include dune.inc)

(executable
 (name omd)
 (libraries str omd)
 (modules omd))

(executable
 (name omd_ocamldoc)
 (libraries str omd)
 (modules omd_ocamldoc))

(rule
 (alias gen)
